{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType":"module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser":true
    },
    "rules": {
        "semi": 2,
        "no-console": 0,
        "no-unused-vars":0,
        "no-useless-escape":0
    }
}